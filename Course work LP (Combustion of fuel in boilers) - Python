Module4.py

from tkinter import *
from tkinter import ttk
import tkinter

from Module1 import *
from Module2 import *
from Module3 import *

# 4. Расчёт выбросов окислов азота.
# Количество окислов азота (в пересчёте на NO2), выбрасываемых в единицу времени (т/год, г/с), рассчитывается по формуле:
# MNO2 = 0,001 * B * Qir * KNO2 * (1-beta),  (7)
def click4():
    window4 = Tk()
    window4.title("Расчёт выбросов окислов азота")
    window4.geometry("1280x620")
    window4["bg"]="gray"

    def show_message4():
        window41 = Tk()
        window41.title("Результат MNO2 и MNOx")
        window41.geometry("400x200")
        window41["bg"] = "gray"

        # Получение данных.
        B = float(B_4.get())
        Qir = float(Qir_4.get())
        KNO2 = float(KNO2_4.get())
        beta = float(beta_4.get())
        nominal = bool(nominal_4.get())
        Qf = float(Qf_4.get())
        Qn = float(Qn_4.get())
        CNOx = float(CNOx_4.get())
        q4 = float(q4_4.get())
        alfayx = float(alfayx_4.get())
        Vr = float(Vr_4.get())

        # Расчёты.
        if nominal:
            MNO2 = 0.001 * B * Qir * KNO2 * (1 - beta)
        else:
            MNO2 = 0.001 * B * Qir * KNO2 * ((Qf/Qn)**0.25) * (1-beta)
        MNO2 = str(MNO2) + " т/год"

        # Значения Vr для некоторых топлив даны в приложении 1. Для газообразного топлива Vr определяется по данным Приложения 2.
        MNOx = "Не вычисляется"
        if CNOx != 0:
            V = Vr * alfayx
            MNOx = 20.4 * CNOx * V * B * (1 - q4/100)
            MNOx = str(MNOx) + " т/год"

        # Вывод результата расчётов.
        label31 = ttk.Label(window41, text='По формуле 7: ' + MNO2)
        label31.place(x=8, y=10)
        label32 = ttk.Label(window41, text='По формуле 8: ' + MNOx)
        label32.place(x=8, y=40)

    # B - расход натурального топлива за рассматриваемый период времени, т/год, тм^3/год, г/с, л/с.
    label20 = ttk.Label(window4, text='B - расход натурального топлива за рассматриваемый период времени, т/год.')
    label20.place(x=8, y=10)
    B_4 = ttk.Entry(window4, font=("arial", 20))
    B_4.place(x=8, y=30)
    B_4.insert(0, "150")

    # Qir - теплота сгорания натурального топлива, МДж/кг, МДж/м^3;
    label21 = ttk.Label(window4, text='Qir - теплота сгорания натурального топлива, МДж/кг.')
    label21.place(x=8, y=80)
    Qir_4 = ttk.Entry(window4, font=("arial", 20))
    Qir_4.place(x=8, y=100)
    Qir_4.insert(0, "4680") # Значение (ккал/кг): Кизеловский бассейн (Приложение 1).

    # KNO2 - параметр, характеризующий количество окислов азота, образующихся на 1 ГДж тепла, кг/ГДж;
    label22 = ttk.Label(window4, text='KNO2 - параметр, характеризующий количество окислов азота, образующихся на 1 ГДж тепла, кг/ГДж.')
    label22.place(x=8, y=150)
    KNO2_4 = ttk.Entry(window4, font=("arial", 20))
    KNO2_4.place(x=8, y=170)
    KNO2_4.insert(0, "5") # Значение (кг/м3): Каменный уголь (Таблица 3).

    # beta - коэффициент, учитывающий степень снижения выбросов окислов азота в результате применения технических решений.
    label23 = ttk.Label(window4, text='beta - коэффициент, учитывающий степень снижения выбросов окислов азота в результате применения технических решений.')
    label23.place(x=8, y=220)
    beta_4 = ttk.Entry(window4, font=("arial", 20))
    beta_4.place(x=8, y=240)
    beta_4.insert(0, "0.6") # < 1.

    # Нагрузка котла номинальная? Да - 1, Нет - 0.
    label24 = ttk.Label(window4, text='Нагрузка котла номинальная? Да - 1, Нет - 0.')
    label24.place(x=8, y=290)
    nominal_4 = ttk.Entry(window4, font=("arial", 20))
    nominal_4.place(x=8, y=320)
    nominal_4.insert(0, "0")

    # Qf - фактическая теплопроизводительность, кВт.
    label25 = ttk.Label(window4, text='Qf - фактическая теплопроизводительность, кВт.')
    label25.place(x=8, y=370)
    Qf_4 = ttk.Entry(window4, font=("arial", 20))
    Qf_4.place(x=8, y=400)
    Qf_4.insert(0, "20")

    # Qn - номинальная теплопроизводительность, кВт.
    label26 = ttk.Label(window4, text='Qn - номинальная теплопроизводительность, кВт.')
    label26.place(x=8, y=450)
    Qn_4 = ttk.Entry(window4, font=("arial", 20))
    Qn_4.place(x=8, y=480)
    Qn_4.insert(0, "25")

    # CNOx - Содержание окислов азота в дымовых газах (%)
    # Значения CNOx (мг/г^3) для маломощных котлов приведены в таблице 3.
    label27 = ttk.Label(window4, text='CNOx - Содержание окислов азота в дымовых газах (%). Если неизвестно, оставьте 0.')
    label27.place(x=780, y=10)
    CNOx_4 = ttk.Entry(window4, font=("arial", 20))
    CNOx_4.place(x=780, y=30)
    CNOx_4.insert(0, "0")

    # q4 - потери тепла вследствие механической неполноты сгорания топлива, %.
    label28 = ttk.Label(window4, text='q4 - потери тепла вследствие механической неполноты сгорания топлива, %')
    label28.place(x=780, y=80)
    q4_4 = ttk.Entry(window4, font=("arial", 20))
    q4_4.place(x=780, y=100)
    q4_4.insert(0, "5") # Значение (%): Каменный уголь (Таблица 2).

    # alfayx
    label29 = ttk.Label(window4, text='alfayx')
    label29.place(x=780, y=150)
    alfayx_4 = ttk.Entry(window4, font=("arial", 20))
    alfayx_4.place(x=780, y=170)
    alfayx_4.insert(0, "1")

    # Vr
    label30 = ttk.Label(window4, text='Vr')
    label30.place(x=780, y=220)
    Vr_4 = ttk.Entry(window4, font=("arial", 20))
    Vr_4.place(x=780, y=240)
    Vr_4.insert(0, "5.61") # Значение (м3/кг): Кизеловский бассейн (Приложение 1).

    # Кнопка для расчёта.
    btn4 = ttk.Button(window4, text="Расчёт", command=show_message4)
    btn4.place(x=8, y=530)

    # Выбор топлива.
    def fuel1():
        Qir_4.delete("0", END)
        Qir_4.insert(0, "4680")
    Fuel1_4 = tkinter.Button(window4, text="Кизеловский бассейн", width=45, command=fuel1)
    Fuel1_4.place(x=350, y=290)

