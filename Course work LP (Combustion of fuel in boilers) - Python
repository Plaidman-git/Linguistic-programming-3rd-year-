Module3.py

from tkinter import *
from tkinter import ttk
import tkinter

from Module1 import *
from Module2 import *
from Module4 import *

# 3. Расчёт выбросов окиси углерода в единицу времени (т/год, г/с) выполняется по формуле:
# Mco = 0,001 * Cco * B * (1 - q4/100), (4)
def click3():
    window3 = Tk()
    window3.title("Расчёт выбросов окиси углерода в единицу времени")
    window3.geometry("1028x450")
    window3["bg"]="gray"

    def show_message3():
        # Настройки окна.
        window31 = Tk()
        window31.title("Результат Mco")
        window31.geometry("400x200")
        window31["bg"] = "gray"

        # Получение данных.
        q4 = float(q4_3.get())
        B = float(B_3.get())
        q3 = float(q3_3.get())
        R = float(R_3.get())
        Qir = float(Qir_3.get())

        # Расчёты.
        # Cco - выход окиси углерода при сжигании топлива, в кг на тонну или на тыс. м^3 топлива. Рассчитывается по формуле:
        # Cco = q3 * R * Qir,    (5)
        Cco = q3/100 * R * Qir
        Mco = 0.001 * Cco * B * (1 - q4/100)
        Mco = str(Mco) + " т/год"

        # Вывод результата расчётов.
        label19 = ttk.Label(window31, text='По формуле 4: ' + Mco)
        label19.place(x=8, y=10)

    # B - расход топлива, т/год, тм^3/год, г/с, л/с.
    label14 = ttk.Label(window3, text='B - расход топлива, т/год')
    label14.place(x=8, y=10)
    B_3 = ttk.Entry(window3, font=("arial", 20))
    B_3.place(x=8, y=30)
    B_3.insert(0, "150")

    # q3 - потери тепла вследствие химической неполноты сгорания топлива, %.
    label15 = ttk.Label(window3, text='q3 - потери тепла вследствие химической неполноты сгорания топлива, %')
    label15.place(x=8, y=80)
    q3_3 = ttk.Entry(window3, font=("arial", 20))
    q3_3.place(x=8, y=100)
    q3_3.insert(0, "1") #Значение: Каменный уголь 0.5-1 (таблица 2).

    # R - коэффициент, учитывающий долю потери тепла вследствие химической неполноты сгорания топлива, обусловленную наличием в продуктах неполного сгорания окиси углерода, принимается для твёрдого топлива - 1,0; газа - 0,5; мазута - 0,65;
    label16 = ttk.Label(window3, text='R - коэффициент потери доли тепла.')
    label16.place(x=8, y=150)
    R_3 = ttk.Entry(window3, font=("arial", 20))
    R_3.place(x=8, y=170)
    R_3.insert(0, "0.65")

    # Qir - низшая теплота сгорания натурального топлива, МДж/кг, МДж/м^3;
    label17 = ttk.Label(window3, text='Qir - низшая теплота сгорания натурального топлива, МДж/кг, МДж/м^3')
    label17.place(x=8, y=220)
    Qir_3 = ttk.Entry(window3, font=("arial", 20))
    Qir_3.place(x=8, y=240)
    Qir_3.insert(0, "4680") # Значение (4680 ккал/кг): Кизеловский бассейн (Приложение 1).

    # q4 - потери тепла вследствие механической неполноты сгорания топлива, %.
    label18 = ttk.Label(window3, text='q4 - потери тепла вследствие механической неполноты сгорания топлива, %')
    label18.place(x=8, y=290)
    q4_3 = ttk.Entry(window3, font=("arial", 20))
    q4_3.place(x=8, y=310)
    q4_3.insert(0, "5") # Значение (%): Каменный уголь (таблица 2).

    # Кнопка для расчёта.
    btn3 = ttk.Button(window3, text="Расчёт", command=show_message3)
    btn3.place(x=8, y=360)

    # Выбор топлива.
    def fuel1():
        Qir_3.delete("0", END)
        Qir_3.insert(0, "4680")
    Fuel1_3 = tkinter.Button(window3, text="Кизеловский бассейн", width=40, command=fuel1)
    Fuel1_3.place(x=500, y=10)

