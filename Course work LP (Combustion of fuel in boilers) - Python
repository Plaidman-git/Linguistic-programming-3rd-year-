main.py

from tkinter import *
from tkinter import ttk
import tkinter

from Module1 import click1
from Module2 import click2
from Module3 import click3
from Module4 import click4

# Главное окно:
root = Tk()
root.title("Boiler")
root.geometry("1280x360")
root["bg"]="gray"
label33 = ttk.Label(root, text="Программа для расчёта выбросов при сжигании топлива в котлах производительностью до 30 т/ч.")
label33.place(x=8, y=10)
label34 = ttk.Label(root, text="Разработал студент 407 группы Князев И.С.")
label34.place(x=8, y=30)

# Кнопки в главном окне:
button1 = tkinter.Button(text="Расчёт выбросов твёрдых частиц летучей золы и недогоревшего топлива, выбрасываемых в атмосферу с дымовыми газами котлоагрегата в единицу времени при сжигании твёрдого топлива и мазута (Mtv).", width = 180, command=click1)
button1.place(x=8, y=100)
button2 = tkinter.Button(text="Расчёт выбросов окислов серы в пересчёте на SO2, выбрасываемых в атмосферу с дымовыми газами котлоагрегатов в единицу времени (MSO2 и MSO2H2S).", width = 180, command=click2)
button2.place(x=8, y=150)
button3 = tkinter.Button(text="Расчёт выбросов окиси углерода в единицу времени (Mco).", width = 180, command=click3)
button3.place(x=8, y=200)
button4 = tkinter.Button(text="Расчёт выбросов окислов азота (MNO2 и MNOx).", width = 180, command=click4)
button4.place(x=8, y=250)

root.mainloop()

Module1.py

from tkinter import *
from tkinter import ttk
import tkinter

from Module2 import *
from Module3 import *
from Module4 import *

# 1. Расчёт выбросов твёрдых частиц летучей золы и недогоревшего топлива (т/год, г/с), выбрасываемых в атмосферу с дымовыми газами котлоагрегата в единицу времени при сжигании твёрдого топлива и мазута.
# Mtv = B*Ar*f*(1-h3),    (1)
def click1():
    window1 = Tk()
    window1.title("Расчёт выбросов твёрдых частиц летучей золы и недогоревшего топлива, выбрасываемых в атмосферу с дымовыми газами котлоагрегата в единицу времени при сжигании твёрдого топлива и мазута.")
    window1.geometry("850x460")
    window1["bg"] = "gray"

    # Окошко с результатом 1.
    def show_message1():
        window11 = Tk()
        window11.title("Результат Mtv")
        window11.geometry("400x200")
        window11["bg"] = "gray"

        # Получение данных.
        ayH = float(ayH_1.get())
        GyH = float(GyH_1.get())
        B = float(B_1.get())
        Ar = float(Ar_1.get())
        h3 = float(h3_1.get())

        # Расчёты.
        f = (ayH/ 100) / (100 - GyH) # Бурые и каменные угли: 0.0026 (Таблица 1).
        Mtv = B * Ar * f * (1 - h3/100)
        Mtv = str(Mtv) + " т/год"

        # Вывод результата расчётов.
        label6 = ttk.Label(window11, text='По формуле 1: ' + Mtv)
        label6.place(x=8, y=10)

    # B - расход топлива, т/год, г/с.
    label1 = ttk.Label(window1, text='Расход топлива, т/год')
    label1.place(x=8, y=10)
    B_1 = ttk.Entry(window1, font=("arial", 20))
    B_1.place(x=8, y=30)
    B_1.insert(0, "150")

    # Ar - зольность топлива на рабочую массу, %.
    label2 = ttk.Label(window1, text='Зольность топлива на рабочую массу, %')
    label2.place(x=8, y=80)
    Ar_1 = ttk.Entry(window1, font=("arial", 20))
    Ar_1.place(x=8, y=100)
    Ar_1.insert(0, "31") # Значение (%): Кизеловский бассейн (Приложение 1).

    # h3 - доля твёрдых частиц, улавливаемых в золоуловителях.
    label3 = ttk.Label(window1, text='Доля твёрдых частиц, улавливаемых в золоуловителях, %')
    label3.place(x=8, y=150)
    h3_1 = ttk.Entry(window1, font=("arial", 20))
    h3_1.place(x=8, y=170)
    h3_1.insert(0, "55")

    # ayH - доля золы топлива в уносе, %.
    label4 = ttk.Label(window1, text='Доля золы топлива в уносе, %')
    label4.place(x=8, y=220)
    ayH_1 = ttk.Entry(window1, font=("arial", 20))
    ayH_1.place(x=8, y=240)
    ayH_1.insert(0, "60")

    # GyH - содержание горючих в уносе, %.
    label5 = ttk.Label(window1, text='Содержание горючих веществ в уносе, %')
    label5.place(x=8, y=290)
    GyH_1 = ttk.Entry(window1, font=("arial", 20))
    GyH_1.place(x=8, y=310)
    GyH_1.insert(0, "65")

    # Кнопка для расчёта.
    btn1 = ttk.Button(window1, text = "Расчёт", command=show_message1)
    btn1.place(x=8, y=360)

    # Выбор топлива.
    def fuel1():
        Ar_1.delete("0", END)
        Ar_1.insert(0, "31")  # Значение (%): Кизеловский бассейн (Приложение 1).
    Fuel1_1 = tkinter.Button(window1, text = "Кизеловский бассейн", width=60, command=fuel1)
    Fuel1_1.place(x=400, y=10)

    def fuel2():
        Ar_1.delete("0", END)
        Ar_1.insert(0, "29.9")
    Fuel2_1 = tkinter.Button(window1, text = "Челябинский бассейн", width=60, command=fuel2)
    Fuel2_1.place(x=400, y=40)

    def fuel3():
        Ar_1.delete("0", END)
        Ar_1.insert(0, "6.6")
    Fuel3_1 = tkinter.Button(window1, text = "Южно-Уральский бассейн", width=60, command=fuel3)
    Fuel3_1.place(x=400, y=70)

    def fuel4():
        Ar_1.delete("0", END)
        Ar_1.insert(0, "27.6")
    Fuel4_1 = tkinter.Button(window1, text = "Карагандинский бассейн", width=60, command=fuel4)
    Fuel4_1.place(x=400, y=100)

    def fuel5():
        Ar_1.delete("0", END)
        Ar_1.insert(0, "32.6")
    Fuel5_1 = tkinter.Button(window1, text = "Экибастузский бассейн", width=60, command=fuel5)
    Fuel5_1.place(x=400, y=130)

    def fuel6():
