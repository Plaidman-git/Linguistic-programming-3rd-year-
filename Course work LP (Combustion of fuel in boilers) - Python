Module2.py

from tkinter import *
from tkinter import ttk
import tkinter

from Module1 import *
from Module3 import *
from Module4 import *

# 2. Расчёт выбросов окислов серы в пересчёте на SO2 (т/год, т/ч, г/с), выбрасываемых в атмосферу с дымовыми газами котлоагрегатов в единицу времени, выполняется по формуле:
# MSO2 = 0,02*B*Sr*(1-hSO2')*(1-hSO2''), (2)
def click2():
    window2 = Tk()
    window2.title("Расчёт выбросов окислов серы в пересчёте на SO2, выбрасываемых в атмосферу с дымовыми газами котлоагрегатов в единицу времени")
    window2.geometry("1088x450")
    window2["bg"]="gray"

    def show_message2():
        window21 = Tk()
        window21.title("Результат MS04")
        window21.geometry("400x200")
        window21["bg"] = "gray"

        # Получение данных.
        H2S = float(H2S_2.get())
        B = float(B_2.get())
        Sr = float(Sr_2.get())
        dhSO2 = float(dhSO2_2.get())
        d2hSO2 = float(d2hSO2_2.get())

        # Расчёты.
        MSO2 = 0.02*B*Sr*(1-dhSO2)*(1-d2hSO2)
        MSO2 = str(MSO2) + " т/год"
        MSO2H2S = "Не вычисляется"
        if H2S != 0:
            MSO2H2S = 1.88 * 10**(-2) * H2S
            MSO2H2S = str(MSO2H2S) + " т/год"

        # Вывод результата расчётов.
        label12 = ttk.Label(window21, text='По формуле 2: ' + MSO2)
        label12.place(x=8, y=10)
        label13 = ttk.Label(window21, text='По формуле 3: ' + MSO2H2S)
        label13.place(x=8, y=50)

    # B - расход, т/год, т/ч, г/с (твёрдого и жидкого топлива); тыс.м^3/год, тыс.м^3/ч, л/с (газообразного топлива);
    label7 = ttk.Label(window2, text='расход, т/год, т/ч, г/с (твёрдого и жидкого топлива); тыс.м^3/год, тыс.м^3/ч, л/с (газообразного топлива)')
    label7.place(x=8, y=10)
    B_2 = ttk.Entry(window2, font=("arial", 20))
    B_2.place(x=8, y=30)
    B_2.insert(0, "150")

    # Sr - содержание серы в топливе на рабочую массу, %, (для газзобразного топлива в кг/100м^3);
    label8 = ttk.Label(window2, text='Sr - содержание серы в топливе на рабочую массу, %, (для газзобразного топлива в кг/100м^3)')
    label8.place(x=8, y=80)
    Sr_2 = ttk.Entry(window2, font=("arial", 20))
    Sr_2.place(x=8, y=100)
    Sr_2.insert(0, "6.1") # Значение (%): Каменный уголь (Приложение 1).

    # hSO2' - доля окислов серы, связываемых летучей золой топлива, принимается при сжигании сланцев эстонских и ленинградских равной 0,8; остальных сланцев - 0,5; для углей Канско-Ачинского бассейна - 0,2 (для березовских - 0,5); для торфа - 0,15; экибастузских углей - 0,02; прочих углей - 0,1; мазута - 0,02; газа - 0,0;
    label9 = ttk.Label(window2, text='доля окислов серы, связываемых летучей золой топлива')
    label9.place(x=8, y=150)
    dhSO2_2 = ttk.Entry(window2, font=("arial", 20))
    dhSO2_2.place(x=8, y=170)
    dhSO2_2.insert(0, "0.8") # < 1.

    # hSO2'' - доля окислов серы,улавливаемых в золоуловителе, принимается равной нулю для сухих золоуловителей, для мокрых - в зависимости от щелочности орошающей воды.
    label10 = ttk.Label(window2, text='доля окислов серы,улавливаемых в золоуловителе')
    label10.place(x=8, y=220)
    d2hSO2_2 = ttk.Entry(window2, font=("arial", 20))
    d2hSO2_2.place(x=8, y=240)
    d2hSO2_2.insert(0, "0.6") # < 1.

    # При наличии в топливе сероводорода расчёт выбросов дополнительного количества окислов серы в пересчёте на SO2 ведётся по формуле:
    # MSO2 = 1,88 * 10^(-2) * |H2S|, (3)

    # |H2S| - содержание сероводорода в топливе, %.
    label11 = ttk.Label(window2, text='Содержание сероводорода в топливе, %. Если неизвестно, оставьте 0')
    label11.place(x=8, y=290)
    H2S_2 = ttk.Entry(window2, font=("arial", 20))
    H2S_2.place(x=8, y=310)
    H2S_2.insert(0, "0")

    # Кнопка для расчёта.
    btn2 = ttk.Button(window2, text="Расчёт", command=show_message2)
    btn2.place(x=8, y=360)

    # Выбор топлива.
    def fuel1():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.61")
    Fuel1_2 = tkinter.Button(window2, text="Кизеловский бассейн", width=60, command=fuel1)
    Fuel1_2.place(x=650, y=10)

    def fuel2():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "1")
    Fuel2_2 = tkinter.Button(window2, text="Челябинский бассейн", width=60, command=fuel2)
    Fuel2_2.place(x=650, y=40)

    def fuel3():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.7")
    Fuel3_2 = tkinter.Button(window2, text="Южно-Уральский бассейн", width=60, command=fuel3)
    Fuel3_2.place(x=650, y=70)

    def fuel4():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.8")
    Fuel4_2 = tkinter.Button(window2, text = "Карагандинский бассейн", width=60, command=fuel4)
    Fuel4_2.place(x=650, y=100)

    def fuel5():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.7")
    Fuel5_2 = tkinter.Button(window2, text = "Экибастузский бассейн", width=60, command=fuel5)
    Fuel5_2.place(x=650, y=130)

    def fuel6():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "1.6")
    Fuel6_2 = tkinter.Button(window2, text = "Тургайский бассейн", width=60, command=fuel6)
    Fuel6_2.place(x=650, y=160)

    def fuel7():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.4")
    Fuel7_2 = tkinter.Button(window2, text = "Кузнецкий бассейн", width=60, command=fuel7)
    Fuel7_2.place(x=650, y=190)

    def fuel8():
        Sr_2.delete("0", END)
        Sr_2.insert(0, "0.4")
    Fuel8_2 = tkinter.Button(window2, text = "Горловский бассейн", width=60, command=fuel8)
